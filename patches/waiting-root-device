# commands to be executed in /dev
# cd /dev

mknod sda b 8 0
mknod sda1 b 8 1
mknod sda2 b 8 2
mknod sda3 b 8 3

mknod null c 1 3
mknod zero c 1 5
mknod random c 1 8
mknod urandom c 1 9

mknod ptmx c 5 2
chmod 666 null zero random urandom ptmx

for i in `seq 0 20`; do mknod tty$i c 4 $i; done

mknod vcs c 7 0
mknod vcs1 c 7 1

mknod vcsa c 7 128
mknod vcsa1 c 7 129
mknod vcsa2 c 7 130

exit 0

